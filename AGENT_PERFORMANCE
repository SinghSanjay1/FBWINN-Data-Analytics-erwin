
CREATE OR REPLACE TABLE FACT_AGENT_ADHERENCE_SUMMARY
(
	AGENT_DATE_KEY VARCHAR(64) NULL ,
	AGENT VARCHAR(32) NOT NULL ,
	DATE VARCHAR(32) NOT NULL ,
	DTO_SCHED NUMBER(10,2) NOT NULL ,
	DTO_ACTUAL NUMBER(10,2) NOT NULL ,
	ADHERENCE_IN NUMBER(10,2) NOT NULL ,
	ADHERENCE_SCHED NUMBER(10,2) NOT NULL ,
	CONFORMANCE_ACTUAL NUMBER(10,2) NOT NULL ,
	CONFORMANCE_SCHED NUMBER(10,2) NOT NULL 
);
